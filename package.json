    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "jasmine": "^3.5.0",
    "node-webcrypto-ossl": "^2.0.7",
    "tslint": "^6.0.0",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
