  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf node_modules/ dist/",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-fix": "eslint \"src/**/*.ts\" --fix",
    "build": "tsc -p .",
    "test": "tsc -p . && jasmine --config=jasmine.json"
  },
  "files": [
    "dist/lib",
    "dist/index.d.ts",
    "dist/index.js"
  ],
